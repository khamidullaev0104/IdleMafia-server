    "cors": "^2.8.5",
    "discord.js": "^14.7.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^6.15.0",
    "gravatar": "^1.8.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.13.16",
    "normalize-url": "^5.3.1",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "sharp": "^0.31.3",
    "tesseract.js": "^4.0.2"
